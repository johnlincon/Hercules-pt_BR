#: npc/cities/morocc.txt
# mes "[Syvia]";
msgctxt "Picky Lady#moc"
msgid "[Syvia]"
msgstr ""

#: npc/cities/morocc.txt
# mes "The fields of Rune-Midgard are infested with wild and dangerous monsters..But, you've got to admit a bunch of them are soooo cute!";
msgctxt "Picky Lady#moc"
msgid "The fields of Rune-Midgard are infested with wild and dangerous monsters..But, you've got to admit a bunch of them are soooo cute!"
msgstr ""

#: npc/cities/morocc.txt
# mes "You know, like how Spore sticks out its tongue after you kill it..";
msgctxt "Picky Lady#moc"
msgid "You know, like how Spore sticks out its tongue after you kill it.."
msgstr ""

