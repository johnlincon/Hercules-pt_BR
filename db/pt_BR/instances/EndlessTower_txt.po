msgid "Return to Alberta"
msgstr "Retorno a Alberta"

# switch (select(.@str$, "Enter the Dungeon", "Return to Alberta", "Cancel")) {
#: npc/instances/EndlessTower.txt
#, fuzzy
msgctxt "Tower Protection Stone"
msgid "Cancel"
msgstr "Cancelar"

