#: npc/instances/EndlessTower.txt
#, fuzzy, c-format
msgctxt "Tower Protection Stone"
msgid "Party Name: %s"
msgstr "Nome da festa: %s"

# mesf("Party Leader: %s", strcharinfo(PC_NAME));
#: npc/instances/EndlessTower.txt
#, fuzzy, c-format
msgctxt "Tower Protection Stone"
msgid "Party Leader: %s"
msgstr "Líder do Partido: %s"

# mesf("^0000ff%s ^000000- Reservation Failed!", .@md_name$);
#: npc/instances/EndlessTower.txt
#, fuzzy, c-format
msgctxt "Tower Protection Stone"
