#: npc/airports/lighthalzen.txt
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "[Airport Staff]"
msgstr "[Equipe do Aeroplano]"

# mes("Welcome to the\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid ""
"Welcome to the\rLighthalzen Airport,\rwhere we offer nonstop\rflights to "
"Einbroch, Juno and Hugel."
msgstr ""
"Bem-vindo ao aeroporto de Lighthalzen, onde oferecemos vôos sem escalas para "
"Einbroch, Juno e Hugel."

# if (select("Board the Airship.", "Cancel.") == 1) {
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "Board the Airship."
msgstr "Embarque na Aeronave."

# if (select("Board the Airship.", "Cancel.") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "Cancel."
msgstr "Cancelar."

# mes("The boarding fee is\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid ""
"The boarding fee is\r1,200 zeny, but you can\rwaive the fee if you redeem\ra "
"Free Ticket for Airship."
msgstr ""
"A taxa de embarque é de 1.200 zeny, mas você pode renunciar à taxa se "
"resgatar um bilhete gratuito para a Aeronave."

# if (select("Yes", "No") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "Yes"
msgstr "Sim"

# if (select("Yes", "No") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "No"
msgstr "Não"

# mes("[Airship Staff]");
#: npc/airports/lighthalzen.txt
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "[Airship Staff]"
msgstr "[Equipe do Aeroplano]"

# mes("I'm sorry, but you don't\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "I'm sorry, but you don't\rhave 1,200 zeny to pay\rfor the boarding fee."
msgstr ""
"Sinto muito, mas você não tem 1.200 zeny para pagar a taxa de embarque."

# mes("Thank you and\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "Thank you and\rplease come again."
msgstr "Obrigado e, por favor, venha novamente."

# mes("Have a good day~");
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Airport Staff#lhz_air1a::lhz_airport1"
msgid "Have a good day~"
msgstr "Tenha um bom dia~"

# mes("[Arrival Staff]");
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "[Arrival Staff]"
msgstr "[Chegada do pessoal]"

# mes("Welcome to Lighthalzen Airport.");
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "Welcome to Lighthalzen Airport."
msgstr "Bem-vindo ao Aeroporto de Lighthalzen."

# mes("Please let me guide you to the\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid ""
"Please let me guide you to the\rmain terminal if you are arriving from your "
"flight. Otherwise, please\rboard the departing Airship to reach your "
"intended destination."
msgstr ""
"Por favor, deixe-me guiá-lo até o terminal principal se você estiver "
"chegando de seu vôo. Caso contrário, por favor, embarque na aeronave de "
"partida para chegar ao destino pretendido."

# if (select("Exit to main terminal.", "Cancel.") == 1) {
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "Exit to main terminal."
msgstr "Saída para o terminal principal."

# if (select("Exit to main terminal.", "Cancel.") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "Cancel."
msgstr "Cancelar."

# mes("Once you're in the main terminal, you will need to pay the fee again\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid ""
"Once you're in the main terminal, you will need to pay the fee again\rto "
"board an Airship. You should\ronly exit if Lighthalzen is your intended "
"destination. Shall we\rproceed to the main terminal?"
msgstr ""
"Uma vez no terminal principal, você precisará pagar a taxa novamente para "
"embarcar em um dirigível. Você só deve sair se Lighthalzen for seu destino "
"pretendido. Devemos prosseguir para o terminal principal?"

# if (select("Yes", "No") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "Yes"
msgstr "Sim"

# if (select("Yes", "No") == 1) {
#: npc/airports/lighthalzen.txt
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid "No"
msgstr "Não"

# mes("Alright, thank you\r"
#: npc/airports/lighthalzen.txt
#, fuzzy
msgctxt "Arrival Staff#lhz_air2a::lhz_airport2"
msgid ""
"Alright, thank you\rfor your patronage\rand I hope you have\ra pleasant "
"flight~"
msgstr ""
"Muito bem, obrigado por seu patrocínio e espero que você tenha um vôo "
"agradável~"
