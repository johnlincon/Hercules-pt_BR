#: npc/pre-re/guides/guides_hugel.txt
# mes "Well, we have a weapon shop in the center of village.";
msgctxt "Hugel Guide Granny#huge"
msgid "Well, we have a weapon shop in the center of village."
msgstr ""

#: npc/pre-re/guides/guides_hugel.txt
# mes "But I don't know if there is any weapon that you find useful.";
msgctxt "Hugel Guide Granny#huge"
msgid "But I don't know if there is any weapon that you find useful."
msgstr ""

#: npc/pre-re/guides/guides_hugel.txt
# mes "Yes, I love Hugel brand Red Potions. I haven't tasted Red Potions from any other brands yet...hohoho. ";
msgctxt "Hugel Guide Granny#huge"
msgid "Yes, I love Hugel brand Red Potions. I haven't tasted Red Potions from any other brands yet...hohoho. "
msgstr ""

#: npc/pre-re/guides/guides_hugel.txt
# mes "The tool shop is located in the center of village.";
msgctxt "Hugel Guide Granny#huge"
msgid "The tool shop is located in the center of village."
msgstr ""

