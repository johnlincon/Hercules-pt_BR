#: npc/airports/izlude.txt
msgctxt "::Airship_Staff_izlude"
msgid "[Airship Staff]"
msgstr "[Equipe do Aeroplano]"

# mes("Welcome! Would you like\r"
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid ""
"Welcome! Would you like\rto board the Airship that\rdeparts on the flight "
"wich stops\rin Juno and Rachel?"
msgstr ""
"Seja bem-vindo! Você gostaria de embarcar na aeronave que parte no vôo que "
"pára em Juno e Rachel?"

# if (select("Board the Airship to Juno/Rachel.", "Cancel.") == 1) {
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid "Board the Airship to Juno/Rachel."
msgstr "Embarcar na Aeronave para Juno/Rachel."

# if (select("Board the Airship to Juno/Rachel.", "Cancel.") == 1) {
#: npc/airports/izlude.txt
msgctxt "::Airship_Staff_izlude"
msgid "Cancel."
msgstr "Cancelar."

# mes("The boarding fee is\r"
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid ""
"The boarding fee is\r1,200 zeny. However, this\rcharged is waived if you use"
"\ra Free Ticket for Airship. Now,\rwould you still like to board?"
msgstr ""
"A taxa de embarque é de 1.200 zeny. No entanto, esta taxa é dispensada se "
"você utilizar um bilhete gratuito para o transporte aéreo. Agora, você ainda "
"gostaria de embarcar?"

# if (select("Yes", "No") == 1) {
#: npc/airports/izlude.txt
msgctxt "::Airship_Staff_izlude"
msgid "Yes"
msgstr "Sim"

# if (select("Yes", "No") == 1) {
#: npc/airports/izlude.txt
msgctxt "::Airship_Staff_izlude"
msgid "No"
msgstr "Não"

# mes("I'm sorry, but you don't\r"
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid "I'm sorry, but you don't\rhave 1,200 zeny to pay\rfor the boarding fee."
msgstr ""
"Sinto muito, mas você não tem 1.200 zeny para pagar a taxa de embarque."

# mes("Thank you and\r"
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid "Thank you and\rplease come again."
msgstr "Obrigado e, por favor, venha novamente."

# mes("Have a good day~");
#: npc/airports/izlude.txt
#, fuzzy
msgctxt "::Airship_Staff_izlude"
msgid "Have a good day~"
msgstr "Tenha um bom dia~"
